{
  "tests": [
    {
      "title": "Різниця квадратів",
      "formula": "a^2 - b^2 = (a+b)(a-b)",
      "category": "multiplication"
    },
    {
      "title": "Квадрат суми двох чисел",
      "formula": "(a \\pm b)^2 = a^2 \\pm 2ab + b^2",
      "category": "multiplication"
    },
    {
      "title": "Куб суми двох чисел",
      "formula": "(a \\pm b)^3 = a^3 \\pm 3a^{2}b + 3a{b}^{2} \\pm b^3",
      "category": "multiplication"
    },
    {
       "title": "Сума кубів двох чисел",
       "formula": "a^3 + b^3 = (a + b)(a^2 - ab + b^2)",
