    },
    {
      "title": "Число в степені нуль",
      "formula": "a^0 = 1",
      "category": "exponents"
    },
    {
      "title": "Сума степенів",
      "formula": "a^x \\cdot a^y = a^{x+y}",
      "category": "exponents"
    },
    {
      "title": "Різниця степенів",
      "formula": "\\frac{a^x}{a^y} = a^{x-y}",
      "category": "exponents"
    },
    {
      "title": "Добуток степенів",
      "formula": "{(a^x)}^{y} = a^{xy}",
      "category": "exponents"
    },
    {
      "title": "Число у від'ємному степені",
      "formula": "a^{-x} = \\frac{1}{a^x}",
      "category": "exponents"
    },
    {
      "title": "Основна тригонометрична тотожність",
      "formula": "\\sin^2x + \\cos^2x = 1",
      "category": "trigonometry"
    },
    {
      "title": "Визначення тангенса",
      "formula": "\\tg x = \\frac{\\sin x}{\\cos x}",
      "category": "trigonometry"
    },
    {
      "title": "Визначення котангенса",
      "formula": "\\tg x = \\frac{\\cos x}{\\sin x}",
      "category": "trigonometry"
    },
    {
      "title": "Добуток тангесна і контангенса",
      "formula": "\\tg{x} \\cdot \\ctg{x} = 1",
      "category": "trigonometry"
    },
    {
      "title": "Перехід від тангенса до косинуса",
      "formula": "1 + \\tg^2 x = \\frac{1}{\\cos^2 x}",
      "category": "trigonometry"
    },
    {
      "title": "Перехід від котангенса до синуса",
      "formula": "1 + \\ctg^2 x = \\frac{1}{\\sin^2 x}",
      "category": "trigonometry"
    },
    {
      "title": "Формула подвійного кута для синуса",
      "formula": "\\sin{2x} = 2\\sin{x}\\cos{x}",
      "category": "trigonometry"
    },
    {
      "title": "Пониження степеня синуса",
      "formula": "\\sin^2{x} = \\frac{1-\\cos{2x}}{2}",
      "category": "trigonometry"
    },
    {
      "title": "Пониження степеня косинуса",
      "formula": "\\cos^2{x} = \\frac{1+\\cos{2x}}{2}",
      "category": "trigonometry"
    },
    {
      "title": "Визначення періоду тригонометричної функції",
      "formula": "T = \\frac{T_0}{|k|}",
      "category": "trigonometry"
    },
    {
      "title": "Синус суми двох кутів",
      "formula": "\\sin{(\\alpha+\\beta)} = \\sin{\\alpha} \\cos{\\beta} + \\cos{\\alpha} \\sin{\\beta}",
      "category": "trigonometry"
    },
    {
      "title": "Синус різниці двох кутів",
      "formula": "\\sin{(\\alpha-\\beta)} = \\sin{\\alpha} \\cos{\\beta} - \\cos{\\alpha} \\sin{\\beta}",
      "category": "trigonometry"
    },
    {
      "title": "Косинус суми двох кутів",
      "formula": "\\cos{(\\alpha+\\beta)} = \\cos{\\alpha} \\cos{\\beta} - \\sin{\\alpha} \\sin{\\beta}",
      "category": "trigonometry"
    },
    {
      "title": "Косинус різниці двох кутів",
      "formula": "\\cos{(\\alpha-\\beta)} = \\cos{\\alpha} \\cos{\\beta} + \\sin{\\alpha} \\sin{\\beta}",
      "category": "trigonometry"
    },
    {
      "title": "Формула n-го члена арифметичної прогресії",
      "formula": "a_n = a_1 + d(n + 1)",
      "category": "progressions"
    },
    {
      "title": "Формула суми перших n-членів арифметичної прогресії",
      "formula": "S_n = \\frac{a_1 + a_n}{2} \\cdot n",
      "category": "progressions"
    },
    {
      "title": "Властивість члена арифметичної прогресії (відносно його сусідніх членів)",
      "formula": "a_k = \\frac{a_{k-1} + a_{k+1}}{2}, k = 2,3,4 ..., n-1",
      "category": "progressions"
    },
    {
      "title": "Формула n-го члена геометричної прогресії",
      "formula": "b_n = b_1 \\cdot q^{n-1}",
      "category": "progressions"
    },
    {
      "title": "Формула суми перших n-членів геометричної прогресії",
      "formula": "S_n = \\frac{b_1 (1 - q^n)}{1 - q}, q \\ne 1",
      "category": "progressions"
    }, {
      "title": "Сума членів нескінченно спадної геометричної прогресії",
      "formula": "S = \\frac{b_1}{1-q}, |q| < 1",
      "category": "progressions"
    },
    {
      "title": "Властивість члена геометричної прогресії (відносно його сусідніх членів)",
      "formula": "b_k^2 = b_{k-1} \\cdot b_{k+1}, k = 2,3,4 ..., n-1",
      "category": "progressions"
    }
  ],
